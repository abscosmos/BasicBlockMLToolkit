{
  "program_id": "locale_time",
  "setup_commands": [
    "echo 'Locale and time testing' > locale_test.txt"
  ],
  "trace_commands": [
    "date",
    "date +%Y-%m-%d",
    "date +%H:%M:%S",
    "date --iso-8601",
    "cal",
    "cal 2024",
    "locale",
    "locale -a | head -5",
    "timedatectl status || echo 'timedatectl not available'",
    "uptime",
    "uptime -s || uptime",
    "sleep 0.1",
    "time sleep 0.1"
  ],
  "cleanup_commands": [
    "rm -f locale_test.txt"
  ]
}